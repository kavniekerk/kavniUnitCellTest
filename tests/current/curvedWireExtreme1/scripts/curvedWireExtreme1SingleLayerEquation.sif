!
! curvedWireExtreme1SingleLayerEquation.sif
!
! ElmerSolver input file for solution of MicroMegas Simulation
!


! constants
!!!!!!!!!!!!!!!!!!!!!!!!!! Constants !!!!!!!!!!!!!!!!!!!!!!!!!!!
! constants

Check Keywords Warn

! Set drift distance, E fields, and voltage
! dist: distance in cm
! Edrift: drift field in V/cm
! Etrans: transfer field in V/cm
! deltaV: voltage (in V)

$ldist = 0.01
$udist = 0.05
$Edrift = 1000
$Etrans = 40000
$deltaV = 0

$WTuel = -2000
$WTucp = -400
$WTlcp = 0
$WTlel = 0

! header
Header
  CHECK KEYWORDS Warn
  Mesh DB "." "curvedWireExtreme1SingleLayerEquation"
  Include Path ""
  Results Directory ""
End


! parameters
!!!!!!!!!!!!!!!!!!!!!!!!!! Simulation parameters !!!!!!!!!!!!!!!!!!!!!!!!!!!
! simulation

Simulation
  Max Output Level = 100
  Coordinate System = Cartesian 3D
  Simulation Type = Steady State
  Steady State Max Iterations = 1
  Output File = "curvedWireExtreme1SingleLayerEquation.result"
  Post File = "curvedWireExtreme1SingleLayerEquation.vtu"
  Coordinate Scaling = 1
End

! constants
Constants
  Permittivity Of Vacuum = 8.8542e-12
End


! parameters
!!!!!!!!!!!!!!!!!!!!!!!!!! Solver parameters !!!!!!!!!!!!!!!!!!!!!!!!!!!
! solver

! solver electrostatics calculation
Solver 1
  Equation = Stat Elec Solver
  Variable = Potential
  Variable DOFs = 1
  Procedure = "StatElecSolve" "StatElecSolver"

  Calculate Electric Field = TRUE
  Calculate Electric Flux = TRUE

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 50
  Linear System Abort Not Converged = TRUE
  Linear System Convergence Tolerance = 1.0e-3
  Linear System Preconditioning = ILU1

  Steady State Convergence Tolerance = 1.0e-3
  Adaptive Mesh Refinement = TRUE
  Adaptive Remesh = TRUE
  Adaptive Save Mesh = TRUE
  Adaptive Error Limit = 0.5e-01
End

! solver for magnetic vector potential A [Vs/m] 
Solver 2
    Equation = "MGDynamics"
    Procedure = "MagnetoDynamics" "WhitneyAVSolver" 
    Variable = "AV"
 
    ! Apply Mortar BCs = Logical TRUE
    ! Fix Input Current Density = Logical TRUE
    
    Nonlinear System Convergence Tolerance = 1.0e-3 
    Nonlinear System Max Iterations = 50 
    Nonlinear System Relaxation Factor = 1 
    Newton-Raphson Iteration = Logical TRUE
  
    Linear System Solver = Direct 
    Linear System Direct Method = UMFPACK
    Linear System Symmetric = Logical TRUE
    Linear System Solver = "Iterative"
    Linear System Preconditioning = None
    Linear System Convergence Tolerance = 1.0e-3
    Linear System Residual Output = 100
    Linear System Max Iterations = 50
    Linear System Iterative Method = CG   

    ! CG CGS BiCGStab l TFQMR GMRES GCR
End 

! magnetohydrodynamics calculation
Solver 3
    Equation = "MGDynamicsCalc"
    Procedure = "MagnetoDynamics" "MagnetoDynamicsCalcFields"
    Linear System Symmetric = TRUE
    Potential Variable = "AV"
    ! Apply Mortar BCs = Logical TRUE

    Calculate Magnetic Field Strength = Logical TRUE
    Calculate Current Density = Logical TRUE
    Calculate nodal forces = logical TRUE
    Calculate Magnetic Vector Potential = Logical TRUE
   
    Linear System Solver = Direct 
    Linear System Direct Method = UMFPACK
    Linear System Solver = "Iterative"
    Linear System Preconditioning = None
    Linear System Residual Output = 100
    Linear System Max Iterations = 50
    Linear System Iterative Method = CG
    Steady State Convergence Tolerance = 1.0e-3
    Linear System Convergence Tolerance = 1.0e-3
End

! vtu format for ParaView with faces' ids 
Solver 4
  Equation = Electric Force
  Procedure = "ElectricForce" "StatElecForce"

  Exec Solver = After all
  exec interval = 1

  Equation = "ResultOutput"
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Output File Name = "curvedWireExtreme1SingleLayerEquation_elec_weight."
  Output Format = "VTU"
  Scalar Field 1 = Potential
  Vtu Format = Logical TRUE
  Binary Output = TRUE
  Single Precision = TRUE
  Save Nodal Fields = Logical TRUE
  Save Geometry Ids = TRUE
End


! parameters
!!!!!!!!!!!!!!!!!!!!!!!!!! Model parameters !!!!!!!!!!!!!!!!!!!!!!!!!!!
! simulation
! equation

Equation 1
  Name = "ModelDomain"
  Active Solvers(4) = 1 2 3 4
  Calculate Electric Energy = TRUE
End


! bodies
!!!!!!!!!!!!!!!!!!!!!!!!!! Bodies are here !!!!!!!!!!!!!!!!!!!!!!!!!!!
! physical volumes defined in gmsh

! Wire - physvol_1a_wire - 900000
Body 1
  Target Bodies(1) = 1
  Name = "Wire - 900000"
  Equation = 1
  Material = 3
End

! Wire - physvol_1b_wire - 910000
Body 2
  Target Bodies(1) = 2
  Name = "Wire - 910000"
  Equation = 1
  Material = 3
End

! Wire - physvol_2a_wire - 920000
Body 3
  Target Bodies(1) = 3
  Name = "Wire - 920000"
  Equation = 1
  Material = 3
End

! Wire - physvol_2b_wire - 930000
Body 4
  Target Bodies(1) = 4
  Name = "Wire - 930000"
  Equation = 1
  Material = 3
End

! Detector shell gas - physvol_gas - 960000
Body 5
  Target Bodies(1) = 5
  Name = "Detector shell gas - 960000"
  Equation = 1
  Material = 1
End

! Lower copper electrode - physvol_lower_cp - 990000
Body 6
  Target Bodies(1) = 6
  Name = "Lower copper electrode - 990000"
  Equation = 1
  Material = 2
End


! materials
!!!!!!!!!!!!!!!!!!!!!!!!!! Materials !!!!!!!!!!!!!!!!!!!!!!!!!!!
! materials associated with physical volumes defined in gmsh

! Gas
Material 1
  Name = "Air (room temperature)"
  Viscosity = 1.983e-5
  Heat expansion Coefficient = 3.43e-3
  Heat Conductivity = 0.0257
  Relative Permittivity = 1.00059
  Sound speed = 343.0
  Heat Capacity = 1005.0
  Density = 1.205
End

! Dielectric
! Material 2
!  Name = "Polycarbonate (generic)"
!  Heat expansion Coefficient = 67.0e-6
!  Heat Conductivity = 0.205
